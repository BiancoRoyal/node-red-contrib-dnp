{
  "name": "node-red-contrib-dnp",
  "version": "0.0.3",
  "private": false,
  "description": "The DNP/DNP3 toolbox package for Node-RED.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/biancode/node-red-contrib-dnp.git"
  },
  "dependencies": {
    "debug": "3.1.0",
    "dnp3": "^0.0.1",
    "dnp3-crc": "^1.0.0",
    "underscore": "1.8.3"
  },
  "keywords": [
    "node-red",
    "DNP",
    "DNP3",
    "dnp",
    "dnp3",
    "nodered",
    "iiot",
    "IIoT",
    "biancode",
    "modbus"
  ],
  "author": "Klaus Landsdorf <klaus.landsdorf@bianco-royal.de>",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/biancode/node-red-contrib-dnp/issues"
  },
  "node-red": {
    "nodes": {
      "DNP-Client": "dnp/dnp-client.js",
      "DNP-Server": "dnp/dnp-server.js",
      "DNP-Read": "dnp/dnp-read.js",
      "DNP-Write": "dnp/dnp-write.js"
    }
  },
  "engines": {
    "node": "6.x || 8.x"
  },
  "homepage": "https://github.com/biancode/node-red-contrib-dnp#readme",
  "scripts": {
    "test": "standard --fix && mocha test/* --reporter dot",
    "build": "standard --fix && gulp publish",
    "prepublishOnly": "standard --fix && npm run build && npm test",
    "ci-publish": "ci-publish",
    "release": "standard-version"
  },
  "files": [
    "dnp"
  ],
  "devDependencies": {
    "chai": "^4.1.2",
    "eslint": "^4.10.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-jsx": "^4.0.2",
    "eslint-plugin-mocha": "^4.11.0",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.3.2",
    "gulp-htmlmin": "^3.0.0",
    "gulp-jsdoc3": "^1.0.1",
    "gulp-sequence": "^0.4.6",
    "gulp-uglify": "^3.0.0",
    "jasmine-node": "^1.14.5",
    "js-beautify": "^1.7.4",
    "mocha": "^3.5.3",
    "nock": "^9.0.27",
    "node-red": "^0.17.5",
    "pump": "^1.0.2",
    "should": "^13.1.3",
    "sinon": "^4.1.1",
    "standard": "^10.0.3",
    "standard-version": "^4.2.0",
    "supertest": "^3.0.0",
    "when": "^3.7.8"
  },
  "directories": {
    "doc": "docs",
    "example": "examples",
    "test": "test"
  },
  "standard": {
    "ignore": [
      "node_modules/",
      "examples/",
      "dnp/",
      "docs",
      "src/public/",
      "test"
    ]
  },
  "main": "index.js"
}
